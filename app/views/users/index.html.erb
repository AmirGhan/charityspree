<div class="result-container">
<h1>Search Results</h1>
<table class="search-results">
  <tbody >
      <% for user in @users %>
        <tr>
          <td>
            <%= image_tag(user.avatar, :class => 'result-avatar') %>
          </td>
          <td>
            <a class="result-name" href="/users/<%=user.id%>"><%= user.first_name %>  <%= user.last_name %></a>
          </td>
        </tr>
      <% end %>
  </tbody>
</table>
</div>
